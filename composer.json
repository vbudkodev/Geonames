{
  "name": "michaeldrennen/geonames",
  "description": "A Laravel (php) package that interfaces with the geolocation services on geonames.org.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Drennen",
      "email": "michaeldrennen74@gmail.com"
    }
  ],
  "repositories": [

    {
      "type": "vcs",
      "url": "https://github.com/vbudkodev/RemoteFile"
    }
  ],
  "require": {
    "php": "^8.4",
    "ext-intl": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "laravel/framework": "*",
    "michaeldrennen/remote-file": "dev-master",
    "michaeldrennen/local-file": "^2.0",
    "curl/curl": "^2.2",
    "fabpot/goutte": "^4.0",
    "nesbot/carbon": "*",
    "illuminate/support": "*",
    "illuminate/routing": "*",
    "illuminate/database": "*",
    "symfony/browser-kit": "^7.0",
    "symfony/http-client": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "orchestra/testbench": "^8.0",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "MichaelDrennen\\Geonames\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelDrennen\\Geonames\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "MichaelDrennen\\Geonames\\GeonamesServiceProvider"
      ]
    }
  }
}